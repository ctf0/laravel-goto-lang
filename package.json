{
    "name": "laravel-goto-lang",
    "displayName": "Laravel Goto Lang",
    "description": "Quick jump to lang",
    "version": "0.1.2",
    "publisher": "ctf0",
    "engines": {
        "vscode": "^1.41.0"
    },
    "repository": "https://github.com/ctf0/laravel-goto-lang",
    "icon": "images/logo.png",
    "categories": [
        "Other"
    ],
    "keywords": [
        "PHP",
        "Laravel",
        "goto",
        "file navigation"
    ],
    "activationEvents": [
        "onLanguage:php",
        "onLanguage:blade",
        "onUri"
    ],
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Laravel Goto Lang",
            "properties": {
                "laravel-goto-lang.methods": {
                    "type": "array",
                    "default": [
                        "trans",
                        "trans_choice",
                        "__",
                        "@lang"
                    ],
                    "items": {
                        "type": "string"
                    },
                    "minItems": 1,
                    "uniqueItems": true,
                    "description": "methods to search for"
                }
            }
        }
    },
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.41.0",
        "mocha": "^6.2.2",
        "tslint": "^5.20.0",
        "typescript": "^3.6.4",
        "vscode-test": "^1.2.2"
    },
    "dependencies": {
        "escape-string-regexp": "^2.0.0",
        "fast-glob": "^3.1.1",
        "lodash.debounce": "^4.0.8"
    }
}

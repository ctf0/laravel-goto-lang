{
    "name": "laravel-goto-lang",
    "displayName": "Laravel goto lang",
    "description": "Quick jump to lang",
    "version": "0.0.5",
    "publisher": "ctf0",
    "engines": {
        "vscode": "^1.41.0"
    },
    "repository": "https://github.com/ctf0/laravel-goto-lang.git",
    "icon": "images/logo.png",
    "categories": [
        "Other"
    ],
    "keywords": [
        "PHP",
        "Laravel",
        "goto",
        "file navigation"
    ],
    "activationEvents": [
        "onLanguage:php",
        "onLanguage:blade"
    ],
    "contributes": {},
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.41.0",
        "mocha": "^6.2.2",
        "tslint": "^5.20.0",
        "typescript": "^3.6.4",
        "vscode-test": "^1.2.2"
    },
    "dependencies": {
        "fast-glob": "^3.1.1",
        "lodash.debounce": "^4.0.8"
    }
}

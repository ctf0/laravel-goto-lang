{
    "name": "laravel-goto-lang",
    "displayName": "Laravel goto lang",
    "description": "Quick jump to lang",
    "version": "0.0.2",
    "publisher": "ctf0",
    "engines": {
        "vscode": "^1.19.0"
    },
    "repository": "https://github.com/ctf0/laravel-goto-lang.git",
    "icon": "images/logo.png",
    "categories": [
        "Other"
    ],
    "keywords": [
        "PHP",
        "Laravel",
        "goto",
        "file navigation"
    ],
    "activationEvents": [
        "onLanguage:php",
        "onLanguage:blade"
    ],
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Laravel goto lang configuration",
            "properties": {
                "laravel_goto_lang.locales": {
                    "type": "array",
                    "default": [
                        "en"
                    ],
                    "items": {
                        "type": "string"
                    },
                    "minItems": 1,
                    "uniqueItems": true,
                    "description": "Multiple locales"
                }
            }
        }
    },
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "mocha": "^6.1.4",
        "typescript": "^2.0.3",
        "vscode": "^1.0.0"
    }
}
